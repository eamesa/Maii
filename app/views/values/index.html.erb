<p id="notice"><%= notice %></p>

<h1>Values</h1>

<table>
  <thead>
    <tr>
      <th>Fecha corte</th>
      <th>Precio cierre</th>
      <th>Valor unidad</th>
      <th>Valor fondo</th>
      <th>Part</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @values.each do |value| %>
      <tr>
        <td><%= value.fecha_corte %></td>
        <td><%= value.precio_cierre %></td>
        <td><%= value.valor_unidad %></td>
        <td><%= value.valor_fondo %></td>
        <td><%= value.part_id %></td>
        <td><%= link_to 'Show', value %></td>
        <td><%= link_to 'Edit', edit_value_path(value) %></td>
        <td><%= link_to 'Destroy', value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Value', new_value_path %>
