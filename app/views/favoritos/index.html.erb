<p id="notice"><%= notice %></p>

<h1>Favoritos</h1>

<table>
  <thead>
    <tr>
      <th>Part</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favoritos.each do |favorito| %>
      <tr>
        <td><%= favorito.part_id %></td>
        <td><%= favorito.user_id %></td>
        <td><%= link_to 'Show', favorito %></td>
        <td><%= link_to 'Edit', edit_favorito_path(favorito) %></td>
        <td><%= button_to 'Destroy', favorito, method: :delete, remote: true %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Favorito', new_favorito_path %>
